@model IEnumerable<Financeiro4.Models.trans>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Transações</h2>

<table class="table">
    <tr>
        <th>
            Cliente
        </th>
        <th>
            Tipo
        </th>
        <th>
           Data
        </th>
        <th>
           Valor
        </th>
        <th>
            Crédito
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.clientes.nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tipos.nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.valor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.credito)
        </td>
    </tr>
}

</table>
